{
  "name": "snek",
  "version": "0.1.0",
  "repository": "git@github.com:jlippitt/snek",
  "author": "James Lippitt <james.lippitt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "format": "prettier --write './src/**/*'",
    "lint": "tslint './src/**/*'",
    "start": "http-server ./public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.1.18",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.2",
    "@types/redux": "^3.6.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "styled-components": "^4.3.2"
  }
}
